{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "unit-cost-app": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/unit-cost-app",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/devextreme/dist/css/dx.common.css",
              "node_modules/devextreme/dist/css/dx.material.blue.light.css",
              "node_modules/bootswatch/dist/flatly/bootstrap.min.css",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "src/styles.css"
            ],
            "scripts": [

            ],
            "allowedCommonJsDependencies": [
              "devextreme/core/http_request",
              "devextreme/core/renderer",
              "devextreme/core/dom_adapter",
              "devextreme/core/utils/deferred",
              "devextreme/core/utils/ajax",
              "devextreme/core/utils/ready_callbacks",
              "devextreme/core/utils/common",
              "devextreme/ui/tree_list",
              "devextreme/ui/validation_group",
              "devextreme/ui/validation_summary",
              "devextreme/ui/tree_view",
              "devextreme/ui/box",
              "devextreme/ui/load_indicator",
              "devextreme/ui/select_box",
              "devextreme/ui/slider",
              "devextreme/ui/sortable",
              "devextreme/ui/tab_panel",
              "devextreme/ui/tabs",
              "devextreme/ui/tag_box",
              "devextreme/ui/text_box",
              "devextreme/ui/toast",
              "devextreme/ui/toolbar",
              "devextreme/ui/tooltip",
              "devextreme/ui/validator",
              "devextreme/viz/vector_map",
              "devextreme/events/core/events_engine",
              "devextreme/events",
              "devextreme/ui/speed_dial_action",
              "devextreme/ui/slide_out_view",
              "devextreme/ui/scroll_view",
              "devextreme/ui/resizable",
              "devextreme/ui/popup",
              "devextreme/ui/popover",
              "devextreme/viz/pie_chart",
              "devextreme/viz/polar_chart",
              "devextreme/ui/pivot_grid",
              "devextreme/ui/pivot_grid_field_chooser",
              "devextreme/ui/menu",
              "devextreme/ui/nav_bar",
              "devextreme/ui/multi_view",
              "devextreme/ui/map",
              "devextreme/ui/load_panel",
              "devextreme/ui/list",
              "devextreme/viz/linear_gauge",
              "devextreme/ui/gantt",
              "devextreme/ui/gallery",
              "devextreme/viz/funnel",
              "devextreme/ui/form",
              "devextreme/ui/file_manager",
              "devextreme/ui/drop_down_button",
              "devextreme/ui/drawer",
              "devextreme/ui/drop_down_box",
              "devextreme/ui/file_uploader",
              "devextreme/ui/filter_builder",
              "devextreme/ui/html_editor",
              "devextreme/ui/lookup",
              "devextreme/ui/number_box",
              "devextreme/ui/progress_bar",
              "devextreme/ui/radio_group",
              "devextreme/viz/range_selector",
              "devextreme/ui/draggable",
              "devextreme/ui/diagram",
              "devextreme/ui/defer_rendering",
              "devextreme/ui/data_grid",
              "devextreme/ui/context_menu",
              "devextreme/ui/color_box",
              "devextreme/viz/circular_gauge",
              "devextreme/ui/date_box",
              "devextreme/ui/check_box",
              "devextreme/viz/chart",
              "devextreme/ui/button_group",
              "devextreme/ui/calendar",
              "devextreme/ui/button",
              "devextreme/viz/bullet",
              "devextreme/ui/autocomplete",
              "devextreme/viz/bar_gauge",
              "devextreme/ui/action_sheet",
              "devextreme/ui/accordion",
              "devextreme/ui/range_slider",
              "devextreme/ui/recurrence_editor",
              "devextreme/viz/sankey",
              "devextreme/ui/scheduler",
              "devextreme/ui/slide_out",
              "devextreme/viz/sparkline",
              "devextreme/ui/switch",
              "devextreme/ui/text_area",
              "devextreme/ui/responsive_box",
              "devextreme/ui/tile_view",
              "devextreme/viz/tree_map"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "unit-cost-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "unit-cost-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "unit-cost-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "unit-cost-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "unit-cost-app:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "unit-cost-app"
}
